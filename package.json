{
  "name": "polidium",
  "productName": "Polidium",
  "version": "2.0.2",
  "description": "Polid Movie Screen",
  "copyright": "Â© 2019, nekobato",
  "main": "./dist/main/main.js",
  "scripts": {
    "pack": "npm run build && electron-builder",
    "start": "electron .",
    "dev": "npm run build:main && DEBUG=true electron ./dist/main/main.js",
    "serve": "vue-cli-service serve",
    "serve:browser": "NODE_ENV=browser vue-cli-service serve",
    "build": "npm run build:renderer && npm run build:main",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit"
  },
  "build": {
    "appId": "net.nekobato.polidium",
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "public"
    },
    "publish": null,
    "linux": {
      "arch": [
        "ia32",
        "x64"
      ],
      "target": [
        "deb",
        "AppImage",
        "rpm"
      ]
    },
    "win": {
      "icon": "./public/icon.ico",
      "target": [
        "squirrel"
      ]
    },
    "mac": {
      "category": "public.app-category.video",
      "icon": "./public/icon.icns",
      "target": "default"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nekobato/Polidium.git"
  },
  "keywords": [],
  "author": "nekobato",
  "bugs": {
    "url": "https://github.com/nekobato/Polidium/issues"
  },
  "homepage": "https://github.com/nekobato/Polidium",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "24.0.15",
    "@vue/cli-plugin-typescript": "3.8.1",
    "@vue/cli-plugin-unit-jest": "3.8.0",
    "@vue/cli-service": "3.8.4",
    "@vue/test-utils": "1.0.0-beta.20",
    "electron": "5.0.4",
    "electron-builder": "^20.44.1",
    "electron-ipc-mock": "0.0.3",
    "electron-widevinecdm": "7.0.0",
    "electron-widevinecdm-dev": "github:oscartbeaumont-ext/electron-widevinecdm-dev",
    "sass": "1.21.0",
    "sass-loader": "7.1.0",
    "ts-jest": "24.0.2",
    "typescript": "3.5.2",
    "vue": "2.6.10",
    "vue-router": "3.0.6",
    "vue-svg-loader": "0.12.0",
    "vuex": "3.1.1"
  }
}
