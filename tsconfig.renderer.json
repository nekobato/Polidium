{
  "extends": "./tsconfig.json",
  "types": ["webpack-env", "jest"],
  "paths": {
    "@/*": ["src/renderer/*"]
  },
  "file": ["./node_modules/vue-router/types/vue.d.ts", "./node_modules/vuex/types/vue.d.ts"],
